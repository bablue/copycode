const subscriptionId = useParams().subscriptionId;

const isValidSubscriptionId = /^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$/.test(subscriptionId);

if (!isValidSubscriptionId) {
  // Handle invalid subscription ID, e.g., throw an error or redirect to a safe page
  console.error("Invalid subscription ID");
}
